        <activity android:name="com.java.caisijie.HistoryActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.java.caisijie.EntitySearchActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.java.caisijie.EntityActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.java.caisijie.LabelActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.java.caisijie.PeopleActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.java.caisijie.PersonActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.java.caisijie.GatherActivity"
            android:screenOrientation="portrait"/>

    </application>

</manifest>